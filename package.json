{
  "name": "warawaraplazaweb",
  "version": "0.1.0",
  "description": "Displays the WaraWara Plaza (from Pretendo servers) in your browser",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "postinstall": "cd node_modules/@pretendonetwork/mii-js && npm i --include=dev && npx tsc && npx tsc-alias && npm run copy-files && cd ../../..",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Milk-Cool/WaraWaraPlazaWeb.git"
  },
  "keywords": [
    "warawara",
    "plaza",
    "pretendo",
    "web"
  ],
  "author": "Milk_Cool",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Milk-Cool/WaraWaraPlazaWeb/issues"
  },
  "homepage": "https://github.com/Milk-Cool/WaraWaraPlazaWeb#readme",
  "dependencies": {
    "@pretendonetwork/mii-js": "github:PretendoNetwork/mii-js",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.4.0",
    "tga2png": "^0.1.3"
  }
}
